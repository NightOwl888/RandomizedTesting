<Project>
  <PropertyGroup Label="NuGet Package Properties">
    <IsPackable>true</IsPackable>
    <PackageTags>random testing numbers text unicode html regex</PackageTags>
    <Authors>NightOwl888</Authors>
    <RepositoryUrl>https://github.com/NightOwl888/RandomizedTesting/</RepositoryUrl>
    <PackageProjectUrl>https://github.com/NightOwl888/RandomizedTesting/</PackageProjectUrl>
    <PackageIconUrl>https://raw.githubusercontent.com/NightOwl888/RandomizedTesting/main/branding/randomized-testing-icon-128x128.png</PackageIconUrl>
    <PackageIcon>randomized-testing-icon-128x128.png</PackageIcon>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
  </PropertyGroup>
  <ItemGroup>
    <None Include="$(SolutionDir)LICENSE.txt" Pack="true" PackagePath="LICENSE.txt"/>
    <None Include="$(SolutionDir)branding\$(PackageIcon)" Pack="true" PackagePath="$(PackageIcon)"/>
  </ItemGroup>
</Project>